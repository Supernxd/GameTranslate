{
  "name": "gametranslate",
  "version": "1.0.0",
  "description": "游戏翻译",
  "main": "main.js",
  "dependencies": {
    "@babel/plugin-syntax-nullish-coalescing-operator": "^7.8.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.5"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^22.6.0"
  },
  "build": {
    "appId": "com.xxx.app",
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Supernxd/GameTranslate.git"
  },
  "author": "nxd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Supernxd/GameTranslate/issues"
  },
  "homepage": "https://github.com/Supernxd/GameTranslate#readme",
  "babel": {
    "plugins": [
      [
        "@babel/plugin-syntax-nullish-coalescing-operator"
      ]
    ]
  }
}
